;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	helloworld.com admin.helloworld.com (
			      7		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;name server
	IN	NS	helloworld.com.

; PTR list
		IN	PTR	helloworld.com
1		IN	PTR	sip1.helloworld.com
2		IN	PTR sip2.helloworld.com
