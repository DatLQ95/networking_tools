;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	helloworld.com admin.helloworld.com (
			      6		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;name server
	IN	NS	helloworld.com.

; PTR list
1		IN	PTR	ns1.helloworld.com
10		IN	PTR	host1.helloworld.com
2		IN	PTR macos.helloworld.com
